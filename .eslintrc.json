{
	"root": true,
	"env": {
		"browser": true,
		"es2022": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"import"
	],
	"rules": {
		"no-duplicate-imports": "warn",
		"prefer-const": "warn"
	},
	"settings": {
		"import/resolver": {
			"webpack": {
				"extensions": [
					".js"
				]
			}
		}
	}
}
